{
  "name": "nuber-client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "apollo-boost": "^0.3.1",
    "apollo-cache-inmemory": "^1.5.1",
    "apollo-client": "^2.5.1",
    "apollo-link": "^1.2.8",
    "apollo-link-error": "^1.1.7",
    "apollo-link-http": "^1.5.11",
    "apollo-link-state": "^0.4.2",
    "apollo-link-ws": "^1.0.14",
    "apollo-utilities": "^1.2.1",
    "axios": "^0.18.0",
    "google-maps-react": "^2.0.2",
    "graphql": "^14.0.2",
    "react": "^16.6.3",
    "react-apollo": "^2.5.1",
    "react-dom": "^16.6.3",
    "react-facebook-login": "^4.1.1",
    "react-helmet": "^5.2.0",
    "react-router-dom": "^4.3.1",
    "react-scripts-ts": "3.1.0",
    "react-sidebar": "^3.0.2",
    "react-toastify": "^4.4.3",
    "styled-components": "^4.1.1",
    "styled-reset": "^2.0.0",
    "subscriptions-transport-ws": "^0.9.15"
  },
  "scripts": {
    "start": "react-scripts-ts start",
    "prebuild": "rm -rf build",
    "build": "react-scripts-ts build",
    "test": "react-scripts-ts test --env=jsdom",
    "eject": "react-scripts-ts eject",
    "precodegen": "apollo schema:download --endpoint=http://localhost:4000/graphql",
    "codegen": "apollo codegen:generate src/types/api.d.ts --queries='src/**/!(*.local).ts' --addTypename --localSchemaFile schema.json --target typescript --outputFlat",
    "predeploy": "yarn build",
    "deploy": "gh-pages -d build"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/dotenv": "^6.1.0",
    "@types/google-maps-react": "^2.0.1",
    "@types/googlemaps": "^3.30.16",
    "@types/jest": "^24.0.9",
    "@types/node": "^11.10.4",
    "@types/react": "^16.8.6",
    "@types/react-dom": "^16.0.10",
    "@types/react-helmet": "^5.0.7",
    "@types/react-router-dom": "^4.3.1",
    "@types/react-sidebar": "^3.0.0",
    "@types/styled-components": "^4.1.11",
    "gh-pages": "^2.0.1",
    "typescript": "^3.1.6"
  },
  "homepage": "https://chicrock.github.io/nuber-client"
}
